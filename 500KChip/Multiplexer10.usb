blockName = "10-bit Multiplexer"


def Inputs():
    return {'Number': int}


def Outputs():
    return {'M1': bool,
            'M2': bool,
            'M3': bool,
            'M4': bool,
            'M5': bool,
            'M6': bool,
            'M7': bool,
            'M8': bool,
            'M9': bool,
            'M10': bool}


def Compute(inputs, outputs):
    number = inputs['Number']
    for bit in range(10):
        outputs['M' + str(bit + 1)] = bool((number >> bit) & 0x01)
